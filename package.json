{
  "name": "deep-fallback",
  "version": "1.0.3",
  "description": "Deep-fallback makes work with fallbacks enjoyable",
  "main": "./lib/src/index.js",
  "types": "./lib/src/index.d.ts",
  "scripts": {
    "lint": "eslint \"./src/**/*\" && tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "validate": "npm run lint && npm test",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run validate && npm run clean && tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxk096/deep-fallback.git"
  },
  "files": [
    "/lib/src",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "deep",
    "fallback",
    "object",
    "dynamic",
    "nested",
    "proxy",
    "get",
    "default",
    "params"
  ],
  "author": "Max Kanaradze",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxk096/deep-fallback/issues"
  },
  "homepage": "https://github.com/maxk096/deep-fallback#readme",
  "devDependencies": {
    "@types/is-object": "1.0.1",
    "@types/jest": "26.0.15",
    "@types/lodash.get": "4.4.6",
    "@typescript-eslint/eslint-plugin": "4.7.0",
    "@typescript-eslint/parser": "4.7.0",
    "conditional-type-checks": "^1.0.5",
    "eslint": "7.13.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "prettier": "2.1.2",
    "rimraf": "3.0.2",
    "ts-jest": "26.4.4",
    "typescript": "4.0.5"
  },
  "dependencies": {
    "is-object": "1.0.1",
    "is-regexp": "2.1.0",
    "lodash.get": "4.4.2"
  }
