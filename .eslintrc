{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "prettier/@typescript-eslint",
    "plugin:prettier/recommended"
  ],
  "env": {
    "es6": true,
    "jest": true,
    "browser": true,
    "node": true
  },
  "ignorePatterns": ["/lib"],
  "rules": {
    "no-unused-vars": ["error", { "args": "none" }],
    "no-console": "error",
    "@typescript-eslint/no-explicit-any": 0
  }
}
